parameters:
  level: 8
  paths:
    - code

  checkMissingIterableValueType: false

  excludePaths:
    - vendor
    - tests

  reportUnmatchedIgnoredErrors: false

  ignoreErrors:
   - '#with no typehint specified#'
   - '#Variable property access on object#'

  phpstanFunctionalProgramming:
    disallowThrow: false
